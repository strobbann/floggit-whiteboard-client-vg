{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-extraneous-dependencies": [
        "error", {
           "devDependencies": false,
           "optionalDependencies": false,
           "peerDependencies": true,
           "packageDir": "./"
        }
    ]
  }
}
